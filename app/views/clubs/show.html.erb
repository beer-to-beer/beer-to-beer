<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @club.name %>
</p>

<p>
  <strong>Street:</strong>
  <%= @club.address.street %>
</p>
<p>
  <strong>House number:</strong>
  <%= @club.address.house_number %>
</p>
<p>
  <strong>City:</strong>
  <%= @club.address.city %>
</p>
<p>
  <strong>Postal code:</strong>
  <%= @club.address.postal_code %>
</p>
<p>
  <strong>Country:</strong>
  <%= @club.address.country %>
</p>

<p>
  <strong>Url:</strong>
  <%= link_to @club.url, @club.url, :target => "_blank" %>


</p>

<p>
  <strong>User:</strong>
  <%= mail_to User.find(@club.user_id).email, User.find(@club.user_id).email %>
</p>
<p>Veranstalltungen:</p>
<p>
  <% @club.events.map(&:imageURL).each do |p| %>
  <%= image_tag p %> <br/>
  <% end %>
</p>
<%= link_to 'Edit', edit_club_path(@club) %> |
<%= link_to 'Back', clubs_path %>
